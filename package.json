{
  "private": true,
  "name": "@hpcc-js/visualizer",
  "version": "2.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf lib* types dist res/*.js res/*.map",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs dist/index.js -c -m -o res/index.min.js",
    "build": "run-s compile-es6 bundle minimize",
    "watch": "run-p compile-es6-watch bundle-watch",
    "link-hpcc-js": "mklink /J hpcc-js ..\\hpcc-js",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "devDependencies": {
    "@hpcc-js/api": "^2.8.4",
    "@hpcc-js/chart": "^2.24.0",
    "@hpcc-js/codemirror": "^2.11.0",
    "@hpcc-js/codemirror-shim": "^2.13.4",
    "@hpcc-js/common": "^2.19.3",
    "@hpcc-js/comms": "^2.11.0",
    "@hpcc-js/composite": "^2.5.7",
    "@hpcc-js/dgrid": "^2.6.5",
    "@hpcc-js/dgrid-shim": "^2.11.2",
    "@hpcc-js/form": "^2.8.6",
    "@hpcc-js/graph": "^2.11.0",
    "@hpcc-js/layout": "^2.16.3",
    "@hpcc-js/map": "^2.14.1",
    "@hpcc-js/other": "^2.13.6",
    "@hpcc-js/phosphor": "^2.13.6",
    "@hpcc-js/phosphor-shim": "^2.11.2",
    "@hpcc-js/timeline": "^2.7.6",
    "@hpcc-js/tree": "^2.8.4",
    "@hpcc-js/util": "^2.8.3",
    "d3-fetch": "^1.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "rollup": "^1.27.5",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "typescript": "^3.6.4",
    "uglify-js": "^3.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GordonSmith/Visualizer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualizer/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualizer#readme"
}
